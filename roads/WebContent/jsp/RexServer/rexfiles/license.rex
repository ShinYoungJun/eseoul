<Report Title="" Author="" Company="" Category="" Description="" CreatedDate="2009-04-07 11:35:30" LastSavedDate="2016-03-29 14:34:42" Version="2.5.1.148"><FieldInfo><FieldSet ID="1"><Field Name="ADDR1" Type="1" Size="0" Xpath=""/><Field Name="NAME" Type="1" Size="0" Xpath=""/><Field Name="SSN" Type="1" Size="0" Xpath=""/><Field Name="AREA_AFTER" Type="2" Size="0" Xpath=""/><Field Name="MUL_FROMDATE" Type="1" Size="0" Xpath=""/><Field Name="MUL_TODATE" Type="1" Size="0" Xpath=""/><Field Name="JUMYONG_DATES" Type="1" Size="0" Xpath=""/><Field Name="SUM" Type="2" Size="0" Xpath=""/><Field Name="VAT" Type="2" Size="0" Xpath=""/><Field Name="TODAY" Type="1" Size="0" Xpath=""/><Field Name="ADMIN_NO" Type="1" Size="0" Xpath=""/><Field Name="PURPOSE" Type="1" Size="0" Xpath=""/><Field Name="ADDR2" Type="1" Size="0" Xpath=""/><Field Name="MANAGER" Type="1" Size="0" Xpath=""/><Field Name="GU_NM" Type="1" Size="0" Xpath=""/><Field Name="DEPT_NM" Type="1" Size="0" Xpath=""/><Field Name="TEL" Type="1" Size="0" Xpath=""/><Field Name="IMG_PATH" Type="1" Size="0" Xpath=""/><Field Name="ADDR_NEW" Type="1" Size="0" Xpath=""/></FieldSet><FieldSet ID="2"><Field Name="ADMIN_NO" Type="1" Default="" Prompt=""/><Field Name="YEAR" Type="1" Default="" Prompt=""/><Field Name="SEQ" Type="1" Default="" Prompt=""/><Field Name="USER_ID" Type="1" Default="" Prompt=""/><Field Name="SECTION" Type="1" Default="" Prompt=""/></FieldSet><FieldSet ID="3"/><FieldSet ID="4"/></FieldInfo><DataSetting EncryptSQL="0" ConnectionString="Provider=MSDAORA.1;Password=ROADS;User ID=ROADS;Data Source=20.20.20.60:1521/oracle;Persist Security Info=True  " DynamicSQL="0" SQLScriptType="1" RecordType="5" DataPath="" DataRoot="rexpert/rexdataset/rexrow" XslPath="" XsdPath="" CsvPath="" CsvRowSeparator="|@|" CsvColumnSeparator="|*|"><OrderInfos/><FilterInfos/><SPInfo FunctionName=""/><SQL>select ('( ' ||(CASE WHEN C.BON IS NULL OR C.BON = '' THEN (C.ADDR1 || '' || C.ADDR2) ELSE (C.ADDR1||' '||DECODE(C.SAN,'1','산 ',' ')||C.BON||DECODE(C.BU,NULL,'','-'||C.BU)||' '||C.ADDR2) END) || ' )') AS ADDR1, C.NAME      ,substr(REPLACE(C.SSN,'-',''), 0, 6)  || '-' || substr(REPLACE(C.SSN,'-',''), 7, 1) || '******' AS SSN
      , B.AREA_SIZE AS AREA_AFTER
      
      , B.MUL_FROMDATE
      , B.MUL_TODATE
      ,(case when b.section = '1' then (B.MUL_FROMDATE || ' ~ ' || B.MUL_TODATE)  else B.PARTLY_PERIOD end ) AS JUMYONG_DATES
      
      , (select sum from cal_sum_info where  (admin_no,year,seq) in (select admin_no,year,cal_sum_seq from calculation_info  where admin_no = b.admin_no and seq = '{?SEQ}' and year = '{?YEAR}')) as SUM
      , (select vat from cal_sum_info where  (admin_no,year,seq) in (select admin_no,year,cal_sum_seq from calculation_info  where admin_no = b.admin_no and seq = '{?SEQ}' and year = '{?YEAR}')) as VAT
      , TO_CHAR(SYSDATE,'YYYYMMDD') TODAY
      ,(SUBSTR(B.ADMIN_NO,10,11) || ' ' || '호') AS ADMIN_NO
      ,(select class_nm || decode(sort_nm,null,'',',') || sort_nm || decode(kind_nm,null,'',',') || kind_nm
          from REF_PURPOSECODE where section = b.type and code = b.purpose_cd) PURPOSE
      ,((select GU_NM || ' ' || BJ_NM from REF_DONGCODE where BJ_CD=B.BJ_CD and SI_CD=B.SIDO and GU_CD=B.SIGUNGU group by GU_NM, BJ_NM)
        || ' ' || B.BONBUN || DECODE(B.BUBUN,null,'',' ','','-') || B.BUBUN || ' ' || '번지'||' '||'앞 도로(보도)') AS ADDR2
      , F.NAME AS MANAGER
      , (select DISTINCT(gu_nm) from ref_deptcode where org_cd=F.org_cd) as gu_nm
      ,(select dept_nm from ref_deptcode where dept_cd=F.dept_cd) as dept_nm
      , F.TEL,
      'http://98.33.1.88/roadsnas/stamp/'||SUBSTR(B.ADMIN_NO,7,3)||'.png' as IMG_PATH

      ,  C.SI||' '||C.GUN||' '||C.DORO_NM||' '||
	(CASE WHEN C.BD_BON IS NOT NULL OR C.BD_BON&lt;&gt;'' 
		THEN (CASE WHEN C.BD_BU IS NOT NULL OR C.BD_BU&lt;&gt;'' THEN (C.BD_BON||'-'||C.BD_BU||' ')ELSE(C.BD_BON||' ')END)  END)	
	||C.BD_NM||' '||C.BD_DET_NM  AS ADDR_NEW

from PLACE B, APPEAL C, TAXATION_INFO D, User_code F
	where B.admin_no = '{?ADMIN_NO}'
	and B.admin_no = C.admin_no
	and F.user_id = '{?USER_ID}'
	and rownum = 1</SQL><ServerConnection URL="http://localhost/jsp/RexServer/RexService.jsp" DataType=""><Parameter Name="service" ValueType="0" Value="ORACLE" ValueFieldID="-1" ValueFieldIndex="-1"/><Parameter Name="servicetype" ValueType="0" Value="design" ValueFieldID="-1" ValueFieldIndex="-1"/><Parameter Name="Driver" ValueType="0" Value="oracle.jdbc.driver.OracleDriver" ValueFieldID="-1" ValueFieldIndex="-1"/><Parameter Name="Connection" ValueType="0" Value="jdbc:oracle:thin:@localhost:1521:orcl" ValueFieldID="-1" ValueFieldIndex="-1"/><Parameter Name="User" ValueType="0" Value="roadsdb" ValueFieldID="-1" ValueFieldIndex="-1"/><Parameter Name="Password" ValueType="0" Value="roadsdb" ValueFieldID="-1" ValueFieldIndex="-1"/><Parameter Name="DefaultSchema" ValueType="0" Value="" ValueFieldID="-1" ValueFieldIndex="-1"/><Parameter Name="NLS" ValueType="0" Value="" ValueFieldID="-1" ValueFieldIndex="-1"/></ServerConnection></DataSetting><GlobalStyleManager/><Page Gutter="0" MirrorMargin="0" Orientation="1" PaperSize="2" PaperWidth="2100" PaperHeight="2970" ReportWidth="2100" TopMargin="50" LeftMargin="50" BottomMargin="50" RightMargin="50"/><Page Gutter="0" MirrorMargin="0" Orientation="1" PaperSize="2" PaperWidth="2100" PaperHeight="2970" ReportWidth="2100" TopMargin="50" LeftMargin="50" BottomMargin="50" RightMargin="50"><Section Type="1" Name="보고서머리글" Height="0" Visible="0" BackStyle="0" BackColor="16777215" FillStyle="0" FillColor="0" CanGrow="0" CanShrink="0" ColumnWidth="0" ColumnDirection="0" ColumnSpacing="0" ColumnLayout="0" Repeat="0" UnderlayNext="0" DetailIteration="0" ForcedGH="0" KeepTogether="1" NewColumn="0" NewPage="0" PrintAtBottom="0" PageReset="0" SuppressBlankSection="0" SuppressRepeatedBlankSection="0" FillBlank="0" Exportable="1" ExportLeft="0" ExportTop="0" ExportRight="0" ExportBottom="0" GroupFieldID="-1" GroupFieldIndex="-1" GroupSortingFieldID="-1" GroupSortingFieldIndex="-1" GroupTOCFieldID="-1" GroupTOCFieldIndex="-1"><ListControls Type="0